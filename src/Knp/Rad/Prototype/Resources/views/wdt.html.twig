{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block head %}
    {{ parent() }}
    <script type="text/javascript">
        function showElement(id) {
            document.getElementById(id).style.display = '';
        }
        function hideElement(id) {
            document.getElementById(id).style.display = 'none';
        }
    </script>
{% endblock head %}

{% block base64 %}

{% endblock base64 %}

{% block toolbar %}
{% if collector.prototypeController is not null %}
    {% set icon %}
    <img width="20" height="28" alt="MegaMAN" src="data:image/png;base64,{{- block('base64') -}}" />
    {% endset %}
    {% set text %}
    <p>
        <span class="sf-toolbar-info-piece-additional-detail sf-toolbar-status sf-toolbar-status-green">
            Your <abbr title="{{ collector.prototypeController }}">controller</abbr> is a <b>Prototype</b>
        </span>
    </p>
    <p>
        Following methods are available
        {% for method in collector.methods %}
            <div>
                <b>&gt;</b>
                <abbr title="{{ method.documentation.context.namespace }}\{{ method.documentation.context.lsen }}::{{ method.method }}">
                    {{ method.alias }}
                </abbr>
            </div>
        {% endfor %}
    </p>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endif %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon">
        <img width="26" height="28" alt="Prototypes" src="data:image/png;base64,{{ block('base64') }}" />
    </span>
    <strong>Prototype Methods</strong>
</span>
{% endblock %}

{% block panel %}
    {% set domains = [] %}
    {% for method in collector.methods if method.domain not in domains %}
        {% set domains = domains|merge([method.domain]) %}
    {% endfor %}

    <h2>Prototype Methods</h2>
    <table>
        {% for domain in domains %}
            <thead>
                <tr>
                    <th colspan="3" style="background: #B3DBD2">
                        DOMAIN {{ domain|upper }}
                    </th>
                </tr>
                <tr>
                    <th>Method</th>
                    <th>Location</th>
                    <th>Documentation</th>
                </tr>
            </thead>
            <tbody>
            {% for method in collector.methods if method.domain == domain %}
                {% set argsid    = method.domain ~ '-' ~ method.alias %}
                {% set argsplus  = method.domain ~ '-' ~ method.alias ~ '-plus' %}
                {% set argsminus = method.domain ~ '-' ~ method.alias ~ '-minus' %}
                <tr>
                    <td>{{ method.alias }}</td>
                    <td>{{ method.documentation.context.namespace }}\{{ method.documentation.context.lsen }}::{{ method.method }}</td>
                    <td>
                        {{ method.documentation.shortDescription }}
                        {% if method.documentation.longDescription|trim is not empty %}
                            <img id="{{ argsplus }}" alt="+" src="data:image/gif;base64,R0lGODlhEgASAMQTANft99/v+Ga44bHb8ITG52S44dXs9+z1+uPx+YvK6WC24G+944/M6W28443L6dnu+Ge54v/+/l614P///wAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAACH5BAEAABMALAAAAAASABIAQAVS4DQBTiOd6LkwgJgeUSzHSDoNaZ4PU6FLgYBA5/vFID/DbylRGiNIZu74I0h1hNsVxbNuUV4d9SsZM2EzWe1qThVzwWFOAFCQFa1RQq6DJB4iIQA7" style="display: inline;margin-bottom: -3px;cursor:pointer" height="12" onclick="showElement('{{ argsid }}');hideElement('{{ argsplus }}');showElement('{{ argsminus }}');" />
                            <img id="{{ argsminus }}" alt="-" src="data:image/gif;base64,R0lGODlhEgASAMQSANft94TG57Hb8GS44ez1+mC24IvK6ePx+Wa44dXs92+942e54o3L6W2844/M6dnu+P/+/l614P///wAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAACH5BAEAABIALAAAAAASABIAQAVCoCQBTBOd6Kk4gJhGBCTPxysJb44K0qD/ER/wlxjmisZkMqBEBW5NHrMZmVKvv9hMVsO+hE0EoNAstEYGxG9heIhCADs=" style="display: inline;margin-bottom: -3px;display:none;cursor:pointer" height="12" onclick="hideElement('{{ argsid }}');hideElement('{{ argsminus }}');showElement('{{ argsplus }}');" />
                        {% endif %}
                    </td>
                </tr>
                {% if method.documentation.longDescription|trim is not empty %}
                    <tr id="{{ argsid }}" style="display:none">
                        <td colspan=3>
                            <strong>{{ method.documentation.longDescription|nl2br }}</strong>
                        </td>
                    </tr>
                {% endif %}
            {% endfor %}
            </tbody>
        {% endfor %}
    </table>
{% endblock %}
